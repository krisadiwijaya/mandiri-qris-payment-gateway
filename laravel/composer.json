{
    "name": "mandiri/qris-laravel",
    "description": "Mandiri QRIS Payment Gateway SDK for Laravel",
    "type": "library",
    "keywords": ["mandiri", "qris", "payment", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mandiri\\Qris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mandiri\\Qris\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mandiri\\Qris\\MandiriQrisServiceProvider"
            ],
            "aliases": {
                "MandiriQris": "Mandiri\\Qris\\Facades\\MandiriQris"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
